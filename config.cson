    reopenProjectMenuCount: 10
    restorePreviousWindowsOnStart: "always"
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "dark-one-dark-syntax"
    ]
    titleBar: "custom"
    uriHandlerRegistration: "never"
  editor:
    fontFamily: "Menlo, Prompt, MaterialDesignIcons, master-icons, monospace"
    maxScreenLineLength: 4096
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabLength: 4
    tabType: "soft"
  "exception-reporting":
    userId: "b188c45a-7d6b-486e-a399-20f27d215b8c"
  filesize:
    DisplayGzippedSizeOnPopup: false
    UseDecimal: true
  "find-and-replace":
    liveSearchMinimumCharacters: 1
  "flex-tool-bar":
    reloadToolBarNotification: false
  "fuzzy-finder":
    preserveLastSearch: true
    searchAllPanes: true
  "git-menu":
    contextMenuItems:
      "commit-all": false
      "commit-staged": false
      "create-branch": false
      "delete-branch": false
      "fetch-all": false
      "ignore-changes": false
      "merge-branch": false
      "pull-all": false
      "push-all": false
      refresh: false
      "run-command": false
      "switch-branch": false
      sync: false
      "sync-all": false
      "unignore-changes": false
  "git-plus":
    general:
      showFormat: "fuller"
  github:
    viewChangesForCurrentFileDiffPaneSplitDirection: "right"
  hyperclick:
    darwinTriggerKeys: "altKey"
  "js-hyperclick": {}
  "language-babel":
    allowLocalOverride: true
    autoIndentJSX: true
    emmetCSSAutocomplete: true
    keepFileExtension: true
  "linter-ui-default":
    panelHeight: 300
    showDecorations: false
  "markdown-preview":
    breakOnSingleNewline: true
    useGitHubStyle: true
  "one-dark-ui":
    hideDockButtons: true
  "project-manager":
    prettifyTitle: false
    showPath: false
    sortBy: "title"
  "remote-sync":
    autoHideLogPanel: true
    foldLogPanel: true
    monitorFileAnimation: false
  "sass-autocompile":
    compileCompressed: false
    compileExpanded: true
    compileOnSave: false
    indentWidth: 4
    showAdditionalCompilationInfo: false
    showOldParametersWarning: false
  "spell-check":
    addKnownWords: true
    knownWords: [
      "Winsor"
    ]
    noticesMode: "console"
  "split-diff":
    muteNotifications: true
    turnOffSoftWrap: true
  "symbols-view":
    quickJumpToFileSymbol: false
  tabs:
    displayMruTabList: false
    enableMruTabSwitching: false
  "tool-bar":
    fullWidth: false
    iconSize: "21px"
    useTouchBar: false
  "tree-view": {}
  welcome:
    showOnStartup: false
".applescript.source":
  editor:
    commentEnd: ""
    commentStart: "-- "
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
".coffee.source":
  editor:
    autoIndentOnPaste: true
".css.scss.source":
  "autocomplete-plus":
    extraWordCharacters: "$"
  editor:
    autoIndentOnPaste: true
    nonWordCharacters: "/\\()\"':,.;<>~!#%^&*|+=[]{}`?…"
".html.php.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-"
".js.jsx.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-…"
".js.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!#@%^&*|+=[]{}`?-…"
".plain.text":
  whitespace:
    ensureSingleTrailingNewline: false
".sass.source":
  editor:
    autoIndentOnPaste: true
    nonWordCharacters: "/\\()\"':,.;<>~!#%^&*|+=[]{}`?…"
".shell.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-…$"
".source.vim-snippet":
  editor:
    tabType: "soft"
".text.vim-help":
  editor:
    tabType: "soft"
".json.source":
    editor:
        softWrap: false
