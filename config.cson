"*":
  "Remote-FTP":
    autoUploadOnSave: "never"
    enableTransferNotifications: true
  "atom-ctags":
    disableComplete: true
  "atom-terminal": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "enter"
    enableAutoConfirmSingleSuggestion: false
    enableExtendedUnicodeSupport: true
    minimumWordLength: 2
    useCoreMovementCommands: false
  core:
    allowPendingPaneItems: false
    audioBeep: false
    automaticallyUpdate: false
    disabledPackages: [
      "about"
      "autosave"
      "background-tips"
      "exception-reporting"
      "language-c"
      "language-csharp"
      "language-go"
      "language-java"
      "language-objective-c"
      "metrics"
      "open-on-github"
      "styleguide"
      "timecop"
      "welcome"
      "deprecation-cop"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "bracket-matcher"
      "autocomplete-plus"
      "find-and-replace"
      "command-palette"
      "fuzzy-finder"
      "git-diff"
      "grammar-selector"
      "link"
      "markdown-preview"
      "notifications"
      "settings-view"
      "spell-check"
      "tree-view"
      "project-viewer"
      "snippets"
      "symbols-view"
      "tool-bar"
      "project-manager"
      "atom-ctags"
      "symbols-view-plus"
      "platformio-ide-terminal"
    ]
    reopenProjectMenuCount: 10
    restorePreviousWindowsOnStart: true
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "tinacious-design-syntax"
    ]
    useCustomTitleBar: true
  editor:
    lineHeight: 1.8
    showIndentGuide: true
    softWrap: true
    tabType: "soft"
  "exception-reporting":
    userId: "fc12ebce-06fa-41c7-a18f-a2529e34b4c5"
  "find-and-replace":
    focusEditorAfterSearch: true
    liveSearchMinimumCharacters: 2
  "git-diff":
    showIconsInEditorGutter: true
  "menu-manager":
    showButton: false
  notifications:
    showErrorsInDevMode: true
  "one-dark-ui":
    layoutMode: "Compact"
    tabSizing: "Minimum"
  "platformio-ide-terminal":
    core:
      workingDirectory: "Home"
    style:
      animationSpeed: 0
      fontSize: "13"
      theme: "homebrew"
    toggles:
      runInsertedText: false
  "project-manager":
    alwaysOpenInSameWindow: false
    environmentSpecificProjects: false
    includeGitRepositories: false
    prettifyTitle: true
    savePathsRelativeToHome: false
    showPath: true
    sortBy: "title"
  "project-viewer":
    autohide: false
    customHoverColor: "#d8c13a"
    customSelectedColor: "#d8c13a"
    customTitleColor: "#d8c13a"
    customWidth: 240
    hideHeader: false
    keepContext: false
    onlyIcons: true
    statusBarVisibility: true
  "status-bar": {}
  "symbols-view-plus":
    plusConfigurations:
      symbolsViewTheme: "Modal Panel (like atom/symbols-view)"
      updateProjectTagsOnFileChange: true
  tabs:
    addNewTabsAtEnd: true
    enableMruTabSwitching: false
  "tool-bar":
    position: "Right"
  "tree-view": {}
  welcome:
    showOnStartup: false
