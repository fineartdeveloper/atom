"*":
  "advanced-open-file":
    fuzzyMatch: true
    helmDirSwitch: true
  "atom-autocomplete-php":
    binPhp: "/usr/local/bin/php"
  "atom-beautify":
    executables:
      php:
        path: "/usr/local/bin/php"
      "php-cs-fixer":
        path: "/usr/local/bin/php-cs-fixer"
    general:
      beautifyEntireFileOnSave: false
      loggerLevel: "debug"
      muteAllErrors: true
      muteUnsupportedLanguageErrors: true
    js:
      brace_style: "expand"
      default_beautifier: "Pretty Diff"
      end_with_comma: true
      jslint_happy: true
      space_after_anon_function: true
  "atom-ide-datatip":
    hoverTime: 50
    showDataTipOnMouseMove: false
  "atom-ide-hyperclick":
    darwinTriggerKeys: "altKey"
  "atom-ide-outline":
    foldInitially: true
    initialDisplay: false
  "atom-minify":
    checkOutputFileAlreadyExists: true
  "atom-package-deps":
    ignored: [
      "build-sass"
    ]
  "atom-ternjs":
    snippetsFirst: true
    useSnippets: true
  "atom-typescript":
    allowJS: true
    autocompleteParens: true
    autocompletionSuggestionPriority: 1
    ignoreNonSuggestionSuggestionDiagnostics: true
    ignoreUnusedSuggestionDiagnostics: true
    ignoredDiagnosticCodes: [
      "8002"
      "8003"
      "8004"
      "8006"
      "8008"
      "8010"
    ]
    jsSyntaxScopes: [
      "source.js"
      "source.jsx"
      "javascript"
      "source.flow"
    ]
    preferBuiltinBusySignal: false
    preferBuiltinOccurrenceHighlight: false
    preferBuiltinSigHelp: false
    preferBuiltinTooltips: false
    tooltipPosition: "top"
  "autocomplete-paths":
    ignoreSubmodules: true
    ignoredPatterns: [
      ".git"
    ]
    maxFileCount: 6000
  "autocomplete-plus":
    autoActivationDelay: 50
    backspaceTriggersAutocomplete: true
    extraWordCharacters: ""
    minimumWordLength: 1
  "bracket-matcher":
    highlightMatchingLineNumber: true
  "command-palette":
    preserveLastSearch: true
  core:
    allowPendingPaneItems: false
    audioBeep: false
    automaticallyUpdate: false
    disabledPackages: [
      "language-shellscript"
