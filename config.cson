"*":
  _dart:
    _firstRun: true
    _version: "1.0.1"
  "advanced-open-file":
    fuzzyMatch: true
  "aligner-css": {}
  "atom-autocomplete-php": {}
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnType: false
    "atom-ide-datatip":
      datatipDebounceDelay: 100
    "atom-ide-diagnostics-ui":
      autoVisibility: true
      showDiagnosticTraces: true
      showDirectoryColumn: true
    "atom-ide-terminal":
      allowTransparency: true
      cursorBlink: true
      fontFamily: "Menlo, master-icons"
    use:
      "atom-ide-code-format": "never"
      "atom-ide-code-highlight": "never"
      hyperclick: "never"
  "atom-toolbar":
    visible: false
  "autocomplete-php":
    executablePath: "/usr/local/bin/php"
  "autocomplete-plus":
    autoActivationDelay: 500
    backspaceTriggersAutocomplete: true
    confirmCompletion: "enter"
    minimumWordLength: 1
    provider: "FuzzyProvider"
  "bracket-matcher":
    highlightMatchingLineNumber: true
  "city-lights-ui":
    fontSize: 12
  "command-palette":
    preserveLastSearch: true
  core:
    allowPendingPaneItems: false
    audioBeep: false
    disabledPackages: [
      "deprecation-cop"
      "github"
    ]
    fileSystemWatcher: "atom"
    followSymlinks: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*.cache"
      "*.last-run"
    ]
    packagesWithKeymapsDisabled: [
      "tool-bar"
      "autoflow"
      "bookmarks"
      "bracket-matcher"
      "find-and-replace"
      "fuzzy-finder"
      "git-diff"
      "github"
      "go-to-line"
      "grammar-selector"
      "image-view"
      "markdown-preview"
      "notifications"
      "open-on-github"
      "spell-check"
      "styleguide"
      "symbols-view"
      "tree-view"
      "advanced-open-file"
      "project-manager"
      "sort-lines"
      "change-case"
      "emmet"
      "atom-beautify"
      "sass-autocompile"
      "git-plus"
      "atom-autocomplete-php"
      "split-diff"
      "aligner"
      "hyperclick"
      "ide-haskell"
      "intentions"
    ]
    telemetryConsent: "no"
    titleBar: "hidden"
    uriHandlerRegistration: "always"
  editor:
    fontFamily: "Menlo, master-icons, monospace"
    fontSize: 14
    maxScreenLineLength: 2048
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabType: "soft"
  emmet:
    extensionsPath: "~/.atom/packages/toolkits/emmet"
  "exception-reporting":
    userId: "f214f68a-9799-402b-a53c-c3814c28ba21"
  "find-and-replace":
    liveSearchMinimumCharacters: 1
  "fuzzy-finder":
    searchAllPanes: true
  "git-plus":
    commits:
      verboseCommits: true
    diffs:
      useSplitDiff: true
    general: {}
  hyperclick:
    darwinTriggerKeys: "altKey"
  "ide-css":
    additionalGrammars: [
      "source.css.sass"
      "source.css.tyls"
      "source.css.scss"
      "source.css.less"
    ]
  "ide-html":
    additionalGrammars: [
      "text.html.php.blade"
    ]
  "linter-ui-default":
    hidePanelWhenEmpty: false
    panelHeight: 300
    showPanel: true
    showProviderName: true
  "markdown-preview":
    breakOnSingleNewline: true
    openPreviewInSplitPane: false
    useGitHubStyle: true
  "one-dark-ui":
    hideDockButtons: true
  "project-manager":
    showPath: false
    sortBy: "title"
  "remote-sync":
    foldLogPanel: true
  "sass-autocompile":
    compileOnSave: false
  "split-diff":
    ignoreWhitespace: true
    syncHorizontalScroll: true
    turnOffSoftWrap: true
  "symbols-view":
    quickJumpToFileSymbol: false
  tabs:
    displayMruTabList: false
    enableMruTabSwitching: false
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
    nonWordCharacters: "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-"
".css.scss.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!#%^&*|+=[]{}`?…-"
".css.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!#%^&*|+=[]{}`?…-"
".sass.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!#%^&*|+=[]{}`?…-"
