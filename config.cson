    projectHome: "~/.dotfiles/projects"
    restorePreviousWindowsOnStart: "always"
    telemetryConsent: "no"
    themes: [
      "goldfish-dark-ui"
      "goldfish-dark-syntax"
    ]
    titleBar: "hidden"
  "double-tag":
    ignoredTags: [
      "area"
      "base"
      "body"
      "br"
      "col"
      "command"
      "embed"
      "head"
      "hr"
      "html"
      "img"
      "input"
      "keygen"
      "link"
      "meta"
      "param"
      "source"
      "title"
      "track"
      "wbr"
      "iframe"
      "script"
    ]
  editor:
    fontFamily: "Hack, master-icons"
    fontSize: 14
    lineHeight: 1.6
    maxScreenLineLength: 8000
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabType: "soft"
  emmet:
    extensionsPath: "~/.dotfiles/emmet"
  "exception-reporting":
    userId: "d909ec40-833e-4cf2-821a-7e8bc3cdba57"
  "find-and-replace":
    liveSearchMinimumCharacters: 1
    searchContextLineCountAfter: 0
    searchContextLineCountBefore: 0
  "ftp-remote-edit":
    config: ""
    password: "1d024c"
    tree:
      showHiddenFiles: true
  "fuzzy-finder":
    excludeVcsIgnoredPaths: false
    preserveLastSearch: true
    searchAllPanes: true
  "goldfish-dark-ui":
    hideDockButtons: true
  hyperclick:
    darwinTriggerKeys: "altKey"
    linuxTriggerKeys: "altKey"
    win32TriggerKeys: "altKey"
  "js-hyperclick":
    extensions: [
      ".js"
      ".json"
      ".node"
      ".ts"
    ]
  "markdown-preview":
    breakOnSingleNewline: true
  "menu-manager":
    showButton: false
  "package-generator":
    packageSyntax: "coffeescript"
  "project-manager":
    savePathsRelativeToHome: true
    showPath: false
    sortBy: "title"
  "remote-ftp":
    connector:
      autoUploadOnSave: "never"
    notifications: {}
    statusbar: {}
    tree: {}
  "remote-sync":
    autoHideLogPanel: true
    foldLogPanel: true
    monitorFileAnimation: false
  "sass-autocompile":
    autoHidePanelDelay: 2000
    compileCompressed: false
    compileExpanded: true
    compileOnSave: false
    notifications: "Notifications"
    showAdditionalCompilationInfo: false
  snippets: {}
  "split-diff":
    ignoreWhitespace: true
    turnOffSoftWrap: true
  "status-bar": {}
  "symbols-view":
    quickJumpToFileSymbol: false
  tabs:
    addNewTabsAtEnd: true
    displayMruTabList: false
    enableMruTabSwitching: false
  "tool-bar": {}
  "tree-view":
    alwaysOpenExisting: true
  welcome:
    showOnStartup: false
  whitespace:
    keepMarkdownLineBreakWhitespace: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
    tabLength: 2
".coffee.source":
  editor:
    autoIndentOnPaste: true
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
".css.scss.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
".html.php.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
".js.source":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
".less":
  aligner:
    config:
      ":-alignment": "left"
".sass.source":
  editor:
    autoIndentOnPaste: true
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
".source.sql":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…"
    softWrap: false
