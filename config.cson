"*":
  "advanced-open-file":
    defaultInputValue: "Empty"
    fuzzyMatch: true
    ignoredPatterns: [
      "*.pyc"
      "*.pyo"
      "*.DS_Store"
      "*.md"
      "*.db"
    ]
  "aligner-javascript":
    ":-alignment": "left"
  "aligner-scss":
    "!-enabled": true
  artisan:
    notifications: false
    php: "/usr/local/bin/php"
  "atom-autocomplete-php":
    gotoKey: "ctrl"
    insertNewlinesForUseStatements: true
  "autocomplete-font-awesome":
    version: "4.7.0"
  "autocomplete-js-import": {}
  "autocomplete-modules": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "enter"
    defaultProvider: "Symbol"
    enableAutoConfirmSingleSuggestion: false
    enableExtendedUnicodeSupport: true
    fileBlacklist: [
      "*.md"
      "*.txt"
    ]
    minimumWordLength: 1
    strictMatching: true
  "autocomplete-snippets": {}
  "bracket-matcher":
    highlightMatchingLineNumber: true
  "chrome-color-picker":
    General:
      preferredFormat: "hex"
      showButtons: true
    HexColors:
      fallbackAlphaFormat: "hsl"
      forceHexSize: "hex6"
      uppercaseHex: true
  colorio:
    hsl: true
    hsv: true
  "command-palette":
    preserveLastSearch: true
  "command-toolbar": {}
  core:
    allowPendingPaneItems: false
    audioBeep: false
    automaticallyUpdate: false
    disabledPackages: [
      "autosave"
      "background-tips"
      "welcome"
      "spell-check"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "archive-view"
      "chrome-color-picker"
      "color-picker"
      "colorio"
      "autoflow"
