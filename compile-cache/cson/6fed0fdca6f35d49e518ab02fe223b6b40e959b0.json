{".text.html":{"Modal":{"prefix":"modal","body":"<button class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#${2:myModal}\">${4:Launch demo modal}</button>\n\n<div id=\"${2:myModal}\" class=\"modal fade\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"${3:myModalLabel}\" aria-hidden=\"true\">\n  <div class=\"modal-dialog${1: modal-lg}\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;</span>\n        </button>\n        <h4 class=\"modal-title\" id=\"${3:myModalLabel}\">Modal title</h4>\n      </div>\n      <div class=\"modal-body\">\n        ${5:...}\n      </div>\n      <div class=\"modal-footer\">\n        ${6:...}\n      </div>\n    </div>\n  </div>\n</div>"}}}